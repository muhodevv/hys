{
  "name": "ecom",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "docker-compose up -d && turbo run dev",
    "stop": "docker-compose down",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.13",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@9.6.7",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}